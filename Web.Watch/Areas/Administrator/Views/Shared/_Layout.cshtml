<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="shortcut icon" href="/Resources/files/wall-clock.png">
    <title>@ViewBag.Title</title>
    @*<script src="http://ajax.googleapis.com/ajax/libs/jquery/1.7.1/jquery.min.js" type="text/javascript"></script>*@

    <script type="text/javascript" src="js/script.js"></script>

    <link href="~/Areas/Administrator/Plugins/Bootrapnew/vendor/fontawesome-free/css/all.min.css" rel="stylesheet" type="text/css">
    <link href="https://fonts.googleapis.com/css?family=Nunito:200,200i,300,300i,400,400i,600,600i,700,700i,800,800i,900,900i" rel="stylesheet">

    <link href="~/Areas/Administrator/Plugins/Bootrapnew/vendor/datatables/dataTables.bootstrap4.min.css" rel="stylesheet">

    <!-- Custom styles for this template-->
    <link href="~/Areas/Administrator/Plugins/Bootrapnew/css/sb-admin-2.min.css" rel="stylesheet">
    <script src="https://kit.fontawesome.com/dcd92f06c4.js" crossorigin="anonymous"></script>

</head>
<body id="page-top">
    <div id="wrapper">
        @Html.Partial("~/Areas/Administrator/Views/Shared/_Sidebar.cshtml")
        <div id="content-wrapper" class="d-flex flex-column">
            <div id="content">
                @Html.Partial("~/Areas/Administrator/Views/Shared/_Header.cshtml")
                <div class="content-wrapper">
                    @RenderBody()
                </div>

            </div>
            @Html.Partial("~/Areas/Administrator/Views/Shared/_Footer.cshtml")
        </div>
    </div>
    <a class="scroll-to-top rounded" href="#page-top">
        <i class="fas fa-angle-up"></i>
    </a>

 
    <script src="~/Areas/Administrator/Plugins/Bootrapnew/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>

    <!-- Core plugin JavaScript-->
    <script src="~/Areas/Administrator/Plugins/Bootrapnew/vendor/jquery-easing/jquery.easing.min.js"></script>

    <!-- Custom scripts for all pages-->

    <script src="~/Areas/Administrator/Plugins/Bootrapnew/js/sb-admin-2.min.js"></script>

    <script src="~/Areas/Administrator/Plugins/Bootrapnew/vendor/datatables/jquery.dataTables.min.js"></script>
    <script src="~/Areas/Administrator/Plugins/Bootrapnew/vendor/datatables/dataTables.bootstrap4.min.js"></script>

    <!-- Page level custom scripts -->
    <script src="~/Areas/Administrator/Plugins/Bootrapnew/js/demo/datatables-demo.js"></script>

    <!-- Page level plugins -->
    <script src="~/Areas/Administrator/Plugins/Bootrapnew/vendor/chart.js/Chart.min.js"></script>

    <!-- Page level custom scripts -->

    <script src="~/Areas/Administrator/Plugins/Bootrapnew/js/demo/chart-pie-demo.js"></script>
</body>
</html>