@using Web.Core.Dto;
@using Web.Watch.Service;
@{
    WebsiteDto website = UtilService.GetWebsite();
}
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="shortcut icon" href="@website.Favicon">
    <title>@ViewBag.Title</title>
    <meta name="title" content="@ViewBag.MetaTitle" />
    <meta name="description" content="@ViewBag.MetaDescription">
    <meta name="robots" content="@ViewBag.MetaRobots">
    <meta name="revisit-after" content="@ViewBag.MetaRevisitAfter">
    <meta http-equiv="content-language" content="@ViewBag.MetaContentLanguage" />
    <meta http-equiv="Content-Type" content="@ViewBag.MetaContentType" />

    <link rel="preconnect" href="https://fonts.gstatic.com">
    <link href="https://fonts.googleapis.com/css2?family=Poppins:wght@100;200;300;400;500;600;700;800;900&display=swap"
          rel="stylesheet">

    <!-- Font Awesome -->
    <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.10.0/css/all.min.css" rel="stylesheet">

    <!-- Libraries Stylesheet -->
    <link href="~/Plugins/Bootrapnew/lib/owlcarousel/assets/owl.carousel.min.css" rel="stylesheet">
    <link href="~/Plugins/Bootrapnew/jquery-ui/jquery-ui.min.css" rel="stylesheet" />
    <!-- Customized Bootstrap Stylesheet -->
    <link href="~/Plugins/Bootrapnew/css/style.css" rel="stylesheet">
    <link href="~/Plugins/Bootrapnew/styleCustom.css" rel="stylesheet">
    <script src="https://kit.fontawesome.com/dcd92f06c4.js" crossorigin="anonymous"></script>
</head>
<body>
    @Html.Partial("~/Views/Shared/_Header.cshtml", website)
    @Html.Partial("~/Views/Shared/_Sidebar.cshtml", website)
    @RenderBody()
    @Html.Partial("~/Views/Shared/_Footer.cshtml", website)

    <a href="#" class="btn btn-danger back-to-top" style="border-radius: 50%;"><i class="fa fa-angle-double-up"></i></a>

    <script src="~/Plugins/Image/lib/jquery-3.5.1.min.js"></script>
    <script src="~/Plugins/Image/src/js/jquery.swipebox.js"></script>

    @*<script src="https://code.jquery.com/jquery-3.4.1.min.js"></script>*@
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/js/bootstrap.bundle.min.js"></script>
    <script src="~/Plugins/Bootrapnew/lib/easing/easing.min.js"></script>
    <script src="~/Plugins/Bootrapnew/lib/owlcarousel/owl.carousel.min.js"></script>
    <script src="~/Plugins/Bootrapnew/jquery-ui/jquery-ui.min.js"></script>
    <!-- Template Javascript -->
    <script src="~/Plugins/Bootrapnew/js/main.js"></script>
</body>
</html>
